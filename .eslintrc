{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "eslint-config-prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module",
    "jsx": true
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "strict": ["error", "never"]
  }
}
